{
  "name": "lukkarimaatti",
  "description": "lukkarimaatti scheduling tool",
  "version": "1.2.8",
  "main": "app/bootstrap.js",
  "private": true,
  "engines": {
    "node": "4.4.2"
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.2.0",
    "baconjs": "0.7.84",
    "bluebird": "3.3.4",
    "body-parser": "1.15.0",
    "browserify": "13.0.0",
    "cheerio": "0.20.0",
    "classnames": "2.2.3",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "date-arithmetic": "3.0.1",
    "dom-helpers": "2.4.0",
    "express": "4.13.4",
    "forever": "0.15.1",
    "invariant": "2.2.1",
    "lodash": "4.9.0",
    "moment": "2.12.0",
    "nodemailer": "2.3.1",
    "path": "0.12.7",
    "pg": "4.5.3",
    "pg-db": "1.0.2",
    "ramda": "0.21.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-overlays": "0.6.3",
    "react-prop-types": "0.2.1",
    "request": "2.70.0",
    "superagent": "1.8.3",
    "uglify-js": "2.6.2",
    "uncontrollable": "3.2.3",
    "warning": "2.1.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "eslint": "^2.6.0",
    "eslint-plugin-react": "4.3.0",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "supervisor": "0.10.0",
    "watch": "0.17.1",
    "watchify": "3.7.0",
    "zombie": "4.2.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build-css": "mkdir -p .generated && cat app/styles/*.css > .generated/style.css",
    "build-js": "browserify --entry app/client.js | uglifyjs --compress --screw-ie8 > .generated/bundle.js",
    "build": "npm run build-css && npm run build-js",
    "watch-css": "watch \"npm run build-css\" app/styles/",
    "watch-js": "watchify app/client.js --debug -o .generated/bundle.js -v",
    "watch": "npm run watch-js & npm run watch-css & supervisor --quiet --ignore node_modules --watch app app/bootstrap.js",
    "start": "node app/bootstrap.js",
    "lint": "eslint app",
    "clean": "rm .generated/*.js .generated/*.css",
    "test": "mocha --delay --compilers js:babelify/node_modules/babel-core/register test/test.js"
  }
}
