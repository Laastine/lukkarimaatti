{
  "name": "lukkarimaatti",
  "description": "lukkarimaatti scheduling tool",
  "version": "1.2.8",
  "main": "app/bootstrap.js",
  "private": true,
  "engines": {
    "node": "4.4.6"
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babelify": "7.3.0",
    "baconjs": "0.7.84",
    "bluebird": "3.4.1",
    "body-parser": "1.15.2",
    "browserify": "13.0.1",
    "cheerio": "0.20.0",
    "classnames": "2.2.5",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "date-arithmetic": "3.1.0",
    "dom-helpers": "2.4.0",
    "express": "4.14.0",
    "forever": "0.15.2",
    "invariant": "2.2.1",
    "lodash": "4.13.1",
    "moment": "2.13.0",
    "nodemailer": "2.4.2",
    "path": "0.12.7",
    "pg": "4.5.5",
    "pg-db": "1.0.2",
    "ramda": "0.21.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-overlays": "0.6.3",
    "react-prop-types": "0.2.1",
    "request": "2.72.0",
    "superagent": "2.0.0",
    "uglify-js": "2.6.4",
    "uncontrollable": "3.2.4",
    "warning": "3.0.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "chai": "3.5.0",
    "eslint": "3.0.0",
    "eslint-plugin-react": "5.2.2",
    "jquery": "3.0.0",
    "mocha": "2.5.3",
    "mocha-phantomjs-core": "2.0.1",
    "node-fetch": "1.5.3",
    "nodemon": "1.9.2",
    "phantomjs-prebuilt": "2.1.7",
    "supervisor": "0.11.0",
    "watch": "0.19.1",
    "watchify": "3.7.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build-css": "mkdir -p .generated && cat app/styles/*.css > .generated/style.css",
    "build-js": "browserify --entry app/client.js | uglifyjs --compress --screw-ie8 > .generated/bundle.js",
    "build": "npm run build-css && npm run build-js",
    "watch-css": "watch \"npm run build-css\" app/styles/",
    "watch-js": "watchify app/client.js --debug -o .generated/bundle.js -v",
    "watch": "NODE_ENV=development npm run watch-js & npm run watch-css & supervisor --quiet --ignore node_modules --watch app app/bootstrap.js",
    "start": "node app/bootstrap.js",
    "lint": "eslint app",
    "clean": "rm .generated/*.js .generated/*.css",
    "test": "node test/integrationTest.js",
    "integration-test": "./node_modules/phantomjs-prebuilt/bin/phantomjs node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js http://localhost:8080/test/runner.html"
  }
}
