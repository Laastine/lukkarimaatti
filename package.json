{
  "name": "lukkarimaatti",
  "description": "lukkarimaatti scheduling tool",
  "version": "1.2.1",
  "main": "app/bootstrap.js",
  "private": true,
  "engines": {
    "node": "4.2.6"
  },
  "dependencies": {
    "babel": "6.3.26",
    "babel-plugin-transform-object-rest-spread": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babelify": "7.2.0",
    "baconjs": "0.7.83",
    "bluebird": "2.10.2",
    "body-parser": "1.14.2",
    "browserify": "13.0.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "express": "4.13.4",
    "forever": "0.15.1",
    "moment": "2.11.1",
    "node-sass": "3.4.2",
    "nodemailer": "2.1.0",
    "path": "0.12.7",
    "pg": "4.4.4",
    "ramda": "0.19.1",
    "react": "0.14.7",
    "react-big-calendar": "0.9.8",
    "react-dom": "0.14.7",
    "request": "2.69.0",
    "superagent": "1.7.2",
    "uglify-js": "2.6.1"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "mocha": "2.4.5",
    "nodemon": "1.8.1",
    "supervisor": "0.9.1",
    "watchify": "3.7.0",
    "zombie": "4.2.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build-css": "node_modules/node-sass/bin/node-sass --output-style=compressed app/scss/style.scss --output .generated",
    "build-js": "node_modules/browserify/bin/cmd.js --entry app/client.js | node_modules/uglify-js/bin/uglifyjs --compress --mangle > .generated/bundle.js",
    "build": "export NODE_ENV=development; npm run build-css && npm run build-js",
    "watch-css": "node_modules/node-sass/bin/node-sass -w app/scss/style.scss --output-style=compressed --output .generated",
    "watch-js": "node_modules/watchify/bin/cmd.js app/client.js --debug -o .generated/bundle.js -v",
    "watch": "npm run watch-js & npm run watch-css & node_modules/supervisor/lib/cli-wrapper.js --quiet --ignore node_modules --watch app app/bootstrap.js",
    "start": "export NODE_ENV=production; node app/bootstrap.js",
    "lint": "eslint app/ ",
    "clean": "rm .generated/*.js .generated/*.css",
    "start-dev": "export NODE_ENV=development; npm run watch",
    "test": "./node_modules/.bin/mocha --delay --compilers js:babelify/node_modules/babel-core/register test/test.js"
  }
}
