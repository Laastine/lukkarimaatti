{
  "name": "lukkarimaatti",
  "description": "lukkarimaatti scheduling tool",
  "version": "1.3.0",
  "main": "app/bootstrap.js",
  "private": true,
  "engines": {
    "node": "6.3.1"
  },
  "dependencies": {
    "axios": "0.13.1",
    "babel": "6.5.2",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babelify": "7.3.0",
    "baconjs": "0.7.85",
    "bluebird": "3.4.1",
    "body-parser": "1.15.2",
    "browserify": "13.1.0",
    "cheerio": "0.20.0",
    "compression": "1.6.2",
    "express": "4.14.0",
    "forever": "0.15.2",
    "moment": "2.14.1",
    "nodemailer": "2.5.0",
    "path": "0.12.7",
    "pg": "4.5.5",
    "pg-db": "1.0.2",
    "ramda": "0.22.1",
    "react": "15.1.0",
    "react-big-calendar": "0.10.3",
    "react-dom": "15.1.0",
    "react-router": "2.6.1",
    "uglify-js": "2.7.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "chai": "3.5.0",
    "eslint": "3.2.2",
    "eslint-plugin-react": "6.0.0",
    "jquery": "3.1.0",
    "lodash": "4.14.2",
    "mocha": "3.0.2",
    "mocha-phantomjs": "4.1.0",
    "phantomjs-prebuilt": "2.1.11",
    "supervisor": "0.11.0",
    "watch": "0.19.1",
    "watchify": "3.7.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build-css": "mkdir -p .generated && cat app/styles/*.css > .generated/style.css",
    "build-js": "browserify --entry app/client.js | uglifyjs --compress --screw-ie8 > .generated/bundle.js",
    "build": "npm run build-css && npm run build-js",
    "watch-css": "watch \"npm run build-css\" app/styles/",
    "watch-js": "watchify app/client.js --debug -o .generated/bundle.js -v",
    "watch": "NODE_ENV=development npm run watch-js & npm run watch-css & supervisor --quiet --ignore node_modules --watch app app/bootstrap.js",
    "start": "node app/bootstrap.js",
    "lint": "eslint app",
    "clean": "rm .generated/*.js .generated/*.css",
    "test": "node test/integrationTest.js",
    "integration-test": "mocha-phantomjs -p node_modules/.bin/phantomjs http://localhost:8080/test/runner.html"
  }
}
